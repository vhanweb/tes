@model IEnumerable<POS.ViewModel.ItemIventoryViewModel>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("ITEM ")
        </th>
        <th>
            @Html.DisplayName("In Stok")
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VariantName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ending)
        </td>
        <td>
            <button type="button" class="btn btn-warning btn-xs btn-ok-item" data-id="@item.VariantID" data-judul="@item.VariantName" data-stok="@item.Ending"><i class="fa fa-check"></i></button>
        </td>
    </tr>
}

</table>
